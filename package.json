{
  "name": "SVGcode",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "vite build && shx echo \"svgco.de\" > docs/CNAME && git add docs/",
    "serve": "vite preview",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:esm",
    "lint:esm": "npx find-unused-exports",
    "lint:js": "npx eslint . --ext .js,.mjs --fix --ignore-pattern docs/",
    "lint:css": "npx stylelint \"**/*.css\" --fix",
    "fix": "npx prettier . --write"
  },
  "dependencies": {
    "browser-fs-access": "^0.16.4",
    "potrace-wasm": "github:tomayac/potrace-wasm#patch-1",
    "svgo": "^2.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-dynamic-import-vars": "^1.1.1",
    "eslint": "^7.24.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.2.0",
    "find-unused-exports": "^1.2.0",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "vite": "^2.1.5",
    "vite-plugin-pwa": "^0.7.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "google",
      "prettier"
    ],
    "rules": {
      "valid-jsdoc": "off"
    }
  }
}
