{
  "name": "SVGcode",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npx eslint . --ext .js,.mjs --fix --ignore-pattern docs/",
    "lint:css": "npx stylelint \"**/*.css\" --fix",
    "fix": "npx prettier . --write"
  },
  "dependencies": {
    "browser-fs-access": "^0.16.4",
    "potrace-wasm": "github:tomayac/potrace-wasm#patch-1",
    "svgo": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "vite": "^2.1.5",
    "vite-plugin-pwa": "^0.6.5"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "google",
      "prettier"
    ],
    "rules": {
      "valid-jsdoc": "off"
    }
  }
}
